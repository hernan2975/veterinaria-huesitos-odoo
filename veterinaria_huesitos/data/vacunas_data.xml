<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Vacunas esenciales para la región pampeana -->
        
        <!-- Vacunas caninas -->
        <record id="vac_polivalente_canina" model="veterinaria_huesitos.vacuna">
            <field name="name">Vacuna Polivalente Canina</field>
            <field name="laboratorio">Laboratorio Veterinario Argentino</field>
            <field name="enfermedades">Moquillo, Hepatitis, Parvovirus, Leptospirosis, Parainfluenza</field>
            <field name="especie_destino">canino</field>
            <field name="dosis_aplicacion">1 ml</field>
            <field name="via_administracion">subcutanea</field>
            <field name="intervalo_refuerzo">21</field>
            <field name="edad_minima">45</field>
            <field name="registro_senasa">SENASA-VAC-12345</field>
            <field name="requiere_refrigeracion" eval="True"/>
            <field name="temperatura_conservacion">2°C a 8°C</field>
        </record>
        
        <record id="vac_antirrabica_canina" model="veterinaria_huesitos.vacuna">
